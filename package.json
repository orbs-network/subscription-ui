{
  "name": "subscription-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vcs.orbs.network",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@orbs-network/orbs-ethereum-contracts-v2": "0.0.21",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.1.0",
    "gh-pages": "^3.1.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.5",
    "moment": "^2.27.0",
    "notistack": "^0.9.17",
    "orbs-pos-data": "^6.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hanger": "^2.2.1",
    "react-hook-form": "^6.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "typechain": "^2.0.0",
    "typescript": "~3.9.7",
    "web3": "^1.2.11",
    "web3-core": "^1.2.11",
    "web3-utils": "^1.2.11"
  },
  "devDependencies": {
    "@typechain/web3-v1": "^1.0.0",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "mkd": "mkdir -p ./src/local && mkdir -p ./ganache-env/_out",
    "postinstall": "npm run generate_types && npm run mkd",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "generate_types": "typechain --target web3-v1 --outDir ./src/contracts './node_modules/@orbs-network/orbs-ethereum-contracts-v2/build/contracts/*.json'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
